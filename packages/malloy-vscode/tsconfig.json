{
  "compilerOptions": {
    "declaration": true,
    "composite": true,
    "rootDir": "./src",
    "noEmit": false,
    "outDir": "out",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": [
      "es2019",
      "es2020.bigint",
      "es2020.string",
      "es2020.symbol.wellknown"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es2019",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "inlineSources": true
  },
  "include": ["./src", "node_modules/us-atlas", "./webpack.config.js"],
	"exclude": [
		"node_modules",
		".vscode-test",
    "out"
	]
}
